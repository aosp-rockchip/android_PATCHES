+static int registerNatives(JNIEnv *env)
+{
+	if (!registerNativeMethods(env, classPathName,
+							   methods, sizeof(methods) / sizeof(methods[0])))
+	{
+		return JNI_FALSE;
+	}
+
+	return JNI_TRUE;
+}
