+    // int   xbitmask = pow(2, HDR2SDR_BIT_DEPTH) - 1;
+    int   ybitmask = pow(2, HDR2SDR_BIT_DEPTH) - 1;
+
+    if (maxMin)
+        maxMin[0] = maxlumi_dst * ybitmask;
+
+    if (maxMin)
+        maxMin[1] = minlumi_dst * ybitmask;
+
+    //makeHDR2SDROETF(500, 0.5);
+    return 0;
+}
