+	jint result = -1;
+	JNIEnv *env = NULL;
+
+	ALOGI("JNI_OnLoad");
+
+	if (vm->GetEnv(&uenv.venv, JNI_VERSION_1_4) != JNI_OK)
+	{
+		ALOGE("ERROR: GetEnv failed");
+		goto bail;
+	}
+	env = uenv.env;
+
+	if (registerNatives(env) != JNI_TRUE)
+	{
+		ALOGE("ERROR: registerNatives failed");
+		goto bail;
+	}
+
+	result = JNI_VERSION_1_4;
+
+bail:
+	return result;
+}
